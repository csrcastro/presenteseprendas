{
	"typescript.preferences.autoImportFileExcludePatterns": [
		"@remix-run/server-runtime",
		"@remix-run/router",
		"express",
		"@radix-ui/**",
		"@react-email/**",
		"react-router-dom",
		"react-router",
		"stream/consumers",
		"node:stream/consumers",
		"node:test",
		"console",
		"node:console"
	],
	"workbench.editorAssociations": {
		"*.db": "sqlite-viewer.view"
	},
	"sqltools.connections": [
		{
			"previewLimit": 50,
			"driver": "SQLite",
			"database": "${workspaceFolder:presenteseprendas}/prisma/data.db",
			"name": "PeP"
		}
	],
	"sqltools.useNodeRuntime": true
}
